# Firebase Environment Variables
# Set these in Firebase Functions Configuration:
# firebase functions:config:set
#
# Example commands:
# firebase functions:config:set app.api_key="your_api_key_here"
# firebase functions:config:set app.database_url="your_database_url_here"
# firebase functions:config:set app.cors_origin="https://anifox-stream.web.app"

# API Configuration
API_KEY=your_api_key_here
DATABASE_URL=your_database_url_here

# CORS Configuration
CORS_ORIGIN=https://anifox-stream.web.app

# Node Environment
NODE_ENV=production

# Firebase Project ID (automatically set)
FIREBASE_PROJECT_ID=anifoxwatch
